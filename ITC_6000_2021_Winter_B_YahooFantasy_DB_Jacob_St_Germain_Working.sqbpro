</sql><sql name="TEST QUERIES">SELECT FirstName, LastName, Username FROM USER
INNER JOIN FANTASY_TEAM ON 
FANTASY_TEAM.Team_ID = USER.FK_Fantasy_Team_ID
WHERE FK_Fantasy_Team_ID =5 ;

SELECT Player_Name, POSITION.Position, Team_Name, Season_Rank, Percent_Rostered, Percent_Changed,Health_Satus,Fantasy_Points FROM PLAYER
INNER JOIN PLAYER_HEALTH_STATUS ON 
PLAYER_HEALTH_STATUS.Player_ID = PLAYER.Player_ID
INNER JOIN HEALTH_STATUS ON 
HEALTH_STATUS.Health_Status_ID= PLAYER_HEALTH_STATUS.Health_Status_ID
INNER JOIN POSITION ON 
POSITION.Position_ID = PLAYER.Position_ID
INNER JOIN PLAYER_NHL_TEAM ON 
PLAYER_NHL_TEAM.NHL_Player_ID = PLAYER.Player_ID
INNER JOIN FANTASY_TEAM_PLAYER ON 
FANTASY_TEAM_PLAYER.Player_ID = PLAYER.Player_ID
INNER JOIN FANTASY_TEAM ON 
FANTASY_TEAM.Team_ID = FANTASY_TEAM_PLAYER.Fantasy_Team_ID
WHERE PLAYER.Player_Name = &quot;Philipp Grubauer&quot; AND Team_Name = &quot;Huskys&quot;;

SELECT League_Name, Username from USER
INNER JOIN LEAGUE ON
LEAGUE.FK_Commish_ID = USER.User_ID;

SELECT League_Type from LEAGUE
INNER JOIN LEAGUE_TYPE ON LEAGUE_TYPE.League_Type_ID = LEAGUE.FK_League_Type_ID
WHERE League_Name = &quot;Dog Lovers&quot;;
 
 SELECT Team_Name,Team_Wins,Points_For,Username FROM FANTASY_TEAM
 INNER JOIN USER on USER.Fantasy_Team_ID = FANTASY_TEAM.Team_ID
 WHERE FANTASY_TEAM.Rank = 1;


SELECT * FROM PLAYER
INNER JOIN PLAYER_GAME_LOG on PLAYER_GAME_LOG.FK_Player_ID = PLAYER.Player_ID
INNER JOIN NHL_MATCHUP ON NHL_MATCHUP.NHL_Matchup_ID = PLAYER_GAME_LOG.FK_NHL_Matchup_ID
WHERE PLAYER_GAME_LOG.G &gt; 0;


SELECT Position , Player_Name, Fantasy_Points FROM PLAYER
INNER JOIN FANTASY_TEAM_PLAYER ON FANTASY_TEAM_PLAYER.Player_ID = PLAYER.Player_ID
INNER JOIN FANTASY_TEAM ON FANTASY_TEAM.Team_ID= FANTASY_TEAM_PLAYER.Fantasy_Team_ID
INNER JOIN POSITION ON POSITION.Position_ID = PLAYER.Position_ID
INNER JOIN LEAGUE ON LEAGUE.League_ID = FANTASY_TEAM.League_ID
WHERE (Team_Name = &quot;Huskys&quot;)
 AND   (League_Name = &quot;Fantasy Hockey League&quot;);</sql><sql name="Create Tables">CREATE TABLE MATCHUP_RESULT (
    Matchup_Result_ID INTEGER,
    Matchup_Result TEXT,
	PRIMARY KEY(&quot;Matchup_Result_ID&quot;)
);

CREATE TABLE FANTASY_TEAM_MATCHUP (
    Team_ID INTEGER,
    Matchup_ID INTEGER,
	FK_Matchup_Result INTEGER,
	FK_Opponent_ID INTEGER,
	Team_Points INTEGER,
	Opponent_Points INTEGER,
	PRIMARY KEY(&quot;Matchup_ID&quot;,&quot;Team_ID&quot;),
	FOREIGN KEY(&quot;Team_ID&quot;) REFERENCES &quot;FANTASY_TEAM&quot;(&quot;Team_ID&quot;),
	FOREIGN KEY(&quot;Matchup_ID&quot;) REFERENCES &quot;MATCHUP&quot;(&quot;Matchup_ID&quot;),
	FOREIGN KEY(&quot;FK_Matchup_Result&quot;) REFERENCES &quot;MATCHUP_RESULT&quot;(&quot;Matchup_Result_ID&quot;),
	FOREIGN KEY(&quot;FK_Opponent_ID&quot;) REFERENCES &quot;FANTASY_TEAM&quot;(&quot;Team_ID&quot;)
